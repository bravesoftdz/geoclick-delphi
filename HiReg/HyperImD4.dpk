{$DESCRIPTION 'THyperImages for Delphi 4'}
{$DESIGNONLY}
{$IMPLICITBUILD OFF}

requires
  vcl40,
  VCLSMP40;

contains
  RoundRectOpt in 'RoundRectOpt.pas' {RoundRectOptionsForm},
  CustomHImages in 'CustomHImages.pas',
  HImages in 'HImages.pas',
  HSEdit in 'HSEdit.pas' {HSEditForm},
  About in 'About.pas' {AboutForm},
  Experts in 'Experts.pas',
  PropEdit in 'PropEdit.pas';

end.
